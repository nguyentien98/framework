{
    "name": "sericode/framework",
    "description": "The Sericode Framework.",
    "type": "library",
    "license": "MIT",
    "homepage": "https://sericode.com",
    "support": {
        "issues": "https://github.com/sericode/framework/issues",
        "source": "https://github.com/sericode/framework"
    },
    "authors": [
        {
            "name": "Nguyen Xuan Quynh",
            "email": "nguyen.xuan.quynh@sericode.com"
        }
    ],
    "require": {
        "php": "^7.2",
        "illuminate/container": "5.7.*",
        "symfony/http-foundation": "^4.1"
    },
    "require-dev": {
        "phpunit/phpunit": "^7.0"
    },
    "autoload": {
        "psr-4": {
            "Sericode\\": "src/Sericode/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Sericode\\Tests\\": "tests/"
        }
    },
    "config": {
        "sort-packages": true
    },
    "minimum-stability": "dev",
    "prefer-stable": true
}
